<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teacher</title>
  </head>
  <body>
    <a href="/">Back</a>
    <br />
    <table border="5">
      <tr>
        <td>ID</td>
        <td>First Name</td>
        <td>Last Name</td>
        <td>Email</td>
      </tr>
      <%
        for(var i = 0;i<dataRows.length;i++) {
          %>
          <tr>
            <td><%= dataRows[i].id %></td>
            <td><%= dataRows[i].first_name %></td>
            <td><%= dataRows[i].last_name %></td>
            <td><%= dataRows[i].email %></td>
          </tr>
          <%
        }
      %>
    </table>
  </body>
</html>
